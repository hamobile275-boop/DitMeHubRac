local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local targetName = "" -- L∆∞u t√™n player nh·∫≠p v√†o
local isFarming = false -- Tr·∫°ng th√°i toggle

-- √î nh·∫≠p t√™n player
local Input = Tab5:Input({
    Title = "Target Player",
    Desc = "Nh·∫≠p t√™n player c·∫ßn b·∫Øn Hitbox",
    Value = "",
    Placeholder = "Enter player name...",
    Callback = function(input) 
        targetName = input
        print("ƒê√£ ƒë·∫∑t target: " .. targetName)
    end
})

-- N√∫t b·∫≠t/t·∫Øt
local Toggle = Tab5:Toggle({
    Title = "Auto Hitbox",
    Desc = "B·∫≠t ƒë·ªÉ li√™n t·ª•c k√≠ch ho·∫°t Hitbox",
    Default = false,
    Callback = function(state) 
        isFarming = state
        print("Auto Hitbox: " .. tostring(isFarming))
    end
})

-- V√≤ng l·∫∑p li√™n t·ª•c
task.spawn(function()
    while task.wait(1) do -- m·ªói 1 gi√¢y
        if isFarming and targetName ~= "" then
            local player = Players:FindFirstChild(targetName)
            if player then
                local eventsFolder = player:FindFirstChild("Events") or player
                for _, obj in ipairs(eventsFolder:GetDescendants()) do
                    if obj:IsA("RemoteEvent") and obj.Name == "Hitbox" then
                        obj:FireServer()
                        print("ƒê√£ k√≠ch ho·∫°t Hitbox:", obj:GetFullName())
                    end
                end
            else
                warn("Kh√¥ng t√¨m th·∫•y player: " .. targetName)
            end
        end
    end
end)

local player = game.Players.LocalPlayer

-- T·∫°o GUI
local gui = Instance.new("ScreenGui")
gui.Name = "CircleButtonGui"
gui.ResetOnSpawn = false
gui.Parent = player:WaitForChild("PlayerGui")

-- N√∫t ·∫£nh tr√≤n
local button = Instance.new("ImageButton")
button.Name = "CircleButton"
button.Size = UDim2.new(0, 60, 0, 60)
button.Position = UDim2.new(0, 100, 0, 100)
button.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- M√†u n·ªÅn ƒëen
button.Image = "rbxassetid://116721573962136" -- Thay ID n·∫øu mu·ªën
button.BackgroundTransparency = 0
button.AutoButtonColor = false
button.Active = true
button.Parent = gui

-- Bo tr√≤n n√∫t
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0.5, 0)
corner.Parent = button

-- Th√™m vi·ªÅn xanh d∆∞∆°ng b√™n ngo√†i
local stroke = Instance.new("UIStroke")
stroke.Color = Color3.fromRGB(0, 120, 255) -- Xanh d∆∞∆°ng
stroke.Thickness = 2
stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
stroke.Parent = button

-- K√©o n√∫t
local dragging = false
local dragInput, mouseStartPos, elementStartPos

button.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		mouseStartPos = input.Position
		elementStartPos = button.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

button.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - mouseStartPos
		button.Position = UDim2.new(
			elementStartPos.X.Scale, elementStartPos.X.Offset + delta.X,
			elementStartPos.Y.Scale, elementStartPos.Y.Offset + delta.Y
		)
	end
end)

-- Click ƒë·ªÉ k√≠ch ho·∫°t ch·ª©c nƒÉng
local isWindowOpen = false

button.MouseButton1Click:Connect(function()
	isWindowOpen = not isWindowOpen
	if isWindowOpen then
		if Window and Window.Open then
			Window:Open()
		end
	else
		if Window and Window.Close then
			Window:Close()
		end
	end
end)
